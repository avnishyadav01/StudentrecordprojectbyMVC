
@model StudentRecordMVC.Models.adminmodel
<h2>SignUP</h2>
@using (Html.BeginForm("signup", "Student", FormMethod.Post))
{
 
    <div class="form-group">
        <label>Enter Email ID</label>
        @Html.TextBoxFor(a => a.Email, new { @class = "form-control", @placeholder = "ENter email ID" })
        @Html.ValidationMessageFor(a => a.Email)
        @ViewData["0"]

    </div>
    <div class="form-group">
        <label>Enter  Passsowrd</label>
        @Html.PasswordFor(a => a.Password, new { @class = "form-control", @placeholder = "ENter  Password" })
        @Html.ValidationMessageFor(a => a.Password)
    </div>
    <div class="form-group">
        <label>COnfirm  PAssword</label>
        @Html.PasswordFor(a => a.Confirmpwd, new { @class = "form-control", @placeholder = "Confirm  Password" })
        @Html.ValidationMessageFor(a => a.Confirmpwd)
    </div>
    <button type="submit">Submit</button>
    <button type="reset">Reset</button>

}
